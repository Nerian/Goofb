<!DOCTYPE html>
<html>
<head>
	<title>Goofb</title>
	<%= stylesheet_link_tag :all %>
	<%= javascript_include_tag :defaults %>
	                                                  	
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>

	<script>
		$(document).ready(function() {
			$("button").button();
		});
	</script>
	
	<%= csrf_meta_tag %>
</head>
<body>
    <div id='wrapper'>
		<div id='header'>
			<ul>
				<li>
					<button><%= link_to '1. Connect to Facebook', home_index_path %></button>
					<button><%= link_to '2. Select the export options', export_new_path %></button>
				</li>
			</ul>
			   
		</div>
		<div id='content'>
			<%= yield %>      
		</div>
	</div>
</body>
</html>
